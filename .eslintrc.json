{
    "extends": "airbnb-base",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "semi": [
            "error",
            "never"
        ],
        "function-paren-newline": [
            "error",
            "consistent"
        ],
        "linebreak-style": "off",
        "no-underscore-dangle": [
            "off"
        ]
    },
    "env": {
        "browser": true,
        "jest/globals": true
    },
    "globals": {
        "lbs": true
    },
    "plugins": ["jest"]
}